    "version": "0.2.0",
    "configurations": [
        {
            "name": "medium",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/elastix",
            "args": [
                "-f",
                "cbct.mha",
                "-m",
                "ct.mhd",
                "-p",
                "params.txt",
                "-out",
                "dir"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/run/medium",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
