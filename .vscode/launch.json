                "-mp",
                "landmarks/01_Moving.txt",
                "-p",
                "${workspaceFolder}/run/learn/params.txt",
                "-out",
                "${workspaceFolder}/run/learn/out",
                "-threads",
                "$OMP_NUM_THREADS",
            ],
            "cwd": "/home/joasiee/instances/learn",
        },
