    "version": "0.2.0",
    "configurations": [
        {
            "name": "learn-1",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/elastix",
            "args": [
                "-f",
                "scans/01_Fixed.nii.gz",
                "-fMask",
                "masks/01_Fixed.nii.gz",
                "-m",
                "scans/01_Moving.nii.gz",
                "-p",
                "${workspaceFolder}/run/learn/params.txt",
                "-out",
                "${workspaceFolder}/run/learn/out",
                "-threads",
                "$OMP_NUM_THREADS",
            ],
            "cwd": "/home/joasiee/Documents/projects/instances/learn",
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "medium",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/elastix",
            "args": [
                "-m",
                "cbct.mha",
                "-f",
                "ct.mhd",
                "-p",
                "params.txt",
                "-out",
                "dir",
                "-threads",
                "$OMP_NUM_THREADS"
            ],
            "cwd": "${workspaceFolder}/run/medium",
        },
        {
            "name": "trivial",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/elastix",
            "args": [
                "-f",
                "fixed.png",
                "-m",
                "moving.png",
                "-p",
                "params.txt",
                "-out",
                "dir",
                "-threads",
                "$OMP_NUM_THREADS"
            ],
            "cwd": "${workspaceFolder}/run/trivial",
        },
        {
            "name": "trivial-mm",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/elastix",
            "args": [
                "-f",
                "fixed.png",
                "-m",
                "moving.png",
                "-p",
                "params.txt",
                "-out",
                "dir",
                "-threads",
                "$OMP_NUM_THREADS"
